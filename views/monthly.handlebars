<div class="row">
	<div class="month"> 
	    <a href="#" id="prev"><aside class="prev">&#10094;</aside></a>
	    <a href="#" id="next"><aside class="next">&#10095;</aside></a>
	    <span class="cal-title">
	      {{ monthToString month }} {{{ year }}}

	    <!-- for some reason month and year appear up here but not down there -->
	    <script>
			console.log({{month}});
			console.log({{year}});
	  	</script>

	    </span>
	</div>

	<div class="calendar weekdays">
	  <div class="weekday-header">Sun</div>
	  <div class="weekday-header">Mon</div>
	  <div class="weekday-header">Tues</div>
	  <div class="weekday-header">Wed</div>
	  <div class="weekday-header">Thurs</div>
	  <div class="weekday-header">Fri</div>
	  <div class="weekday-header">Sat</div>
	</div>

	<div class="calendar"> 
	  {{#days }}	
	  		<div class="day {{#unless this }} null-value {{/unless}}">

	  			<!--Get get month and year to show up, if you hover over a link on the monthly view only the day shows up -->

	  			<a href="/view/{{{month}}}/{{{year}}}/{{this}}">

	  				<script>
	  				//These yeild nothing right now :-(
	  				console.log({{month}});
	  				console.log({{year}});
	  				</script>


			  		<aside class="date-label"><div class="dayOfWeek">{{ getDayOfWeek ../month this ../year }}</div>{{this}}</aside>
			  			{{>eventList events=(getEvents ../month this ../year)}}
			  		</ul>
		  		</a>
		  		
	  		</div>
	  {{/days}}
	  </div>
</div>